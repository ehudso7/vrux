{
  "name": "vrux",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered React component generator with live preview and Tailwind CSS styling",
  "keywords": [
    "react",
    "component-generator",
    "ai",
    "tailwind",
    "ui",
    "nextjs",
    "typescript"
  ],
  "author": {
    "name": "VRUX Team",
    "email": "support@vrux.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ehudso7/vrux.git"
  },
  "bugs": {
    "url": "https://github.com/ehudso7/vrux/issues"
  },
  "homepage": "https://vrux.dev",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:vercel": "npm run build",
    "start": "next start",
    "lint": "next lint",
    "check-env": "node scripts/check-env.js",
    "predev": "npm run check-env",
    "test": "echo \"Tests coming soon\" && exit 0",
    "test:ci": "npm test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "echo 'Setup complete'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "deploy": "node scripts/trigger-deploy.js all",
    "deploy:vercel": "node scripts/trigger-deploy.js vercel",
    "deploy:render": "node scripts/trigger-deploy.js render"
  },
